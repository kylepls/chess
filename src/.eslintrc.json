{
  "extends": "react-app",
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-restricted-imports": ["off"],
    "semi": [
      "error",
      "never"
    ],
    "max-depth": [
      "error",
      5
    ],
    "max-lines": [
      "error",
      333
    ],
    "prefer-destructuring": [
      "error",
      {
        "array": true,
        "object": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  }
}
